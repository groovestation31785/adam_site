  <%= csrf_meta_tags %>
</head>
  <body>
    
    <!-- Wrapper -->
			<div id="wrapper">
			  
        <!-- Header -->
					<header id="header">
						<div class="inner">
						<!-- Logo -->
							<a href="index.html" class="logo">
								<span class="symbol"><img src="images/icon.png" alt="" /></span><span class="title">Adam Frankenfeld</span>
							</a>

						<!-- Nav -->
							<nav>
								<ul>
									<li><a href="#menu">Menu</a></li>
								</ul>
							</nav>
						</div>
					</header>

				<!-- Menu -->
					<nav id="menu">
						<h2>Menu</h2>
  						<ul>
  							<li><a href="index.html">Home</a></li>
  							<li><a href="generic.html">About Me</a></li>
  							<li><a href="generic.html">Beneath the Skin</a></li>
  							<li><a href="generic.html">Christian Drama Vids</a></li>
  							<li><a href="generic.html">African Trauma Radio</a></li>
  							<li><a href="generic.html">The Real Inspector Hound</a></li>
